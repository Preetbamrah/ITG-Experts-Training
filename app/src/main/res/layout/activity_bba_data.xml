<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimary">
        <include
            layout="@layout/toolbar"
            android:id="@+id/action_toolbar"
            />
        <RelativeLayout
            android:id="@+id/marketing_rela"
            android:layout_width="match_parent"
            android:layout_below="@+id/action_toolbar"
            android:layout_height="100dp"
            android:background="#FDFDFD"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/image_marketing"
                android:layout_width="80dp"
                android:layout_height="70dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/marketing"
                android:padding="0dp"
                android:layout_marginLeft="20dp"
                android:layout_marginBottom="20dp"/>
        <TextView
            android:id="@+id/textmarketing"
            android:layout_toRightOf="@+id/image_marketing"
            android:layout_width="160dp"
            android:layout_marginTop="20dp"
            android:textColor="@color/skyblue"
            android:layout_height="wrap_content"
            android:text="MARKETING"
            android:textStyle="bold"
            android:gravity="center"
            android:padding="10dp"
            android:textSize="20dp"
            />
            <ImageView
                android:id="@+id/marketing_sign"
                android:layout_marginTop="28dp"
                android:background="@drawable/circle_background"
                android:backgroundTint="@color/colorPrimary"
                android:tint="#FDFDFD"
                android:layout_width="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_height="40dp"
                android:layout_toRightOf="@id/textmarketing"
                android:src="@drawable/signgreater"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/digital_rela"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#FDFDFD"
            android:layout_below="@+id/marketing_rela"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">
            <ImageView
                android:id="@+id/image_digital"
                android:layout_width="80dp"
                android:layout_marginTop="20dp"
                android:layout_height="70dp"
                android:layout_marginBottom="20dp"
                android:src="@drawable/dmarketing"
                android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/textdigital"
            android:layout_toRightOf="@id/image_digital"
            android:layout_width="160dp"
            android:layout_marginTop="20dp"
            android:textColor="@color/skyblue"
            android:textStyle="bold"
            android:layout_height="wrap_content"
            android:text="DIGITAL MARKETING"
            android:gravity="center"
            android:padding="10dp"
            android:textSize="20dp"
            />
            <ImageView
                android:id="@+id/digital_sign"
                android:layout_marginTop="28dp"
                android:background="@drawable/circle_background"
                android:backgroundTint="@color/colorPrimary"
                android:tint="#FDFDFD"
                android:layout_width="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_height="40dp"
                android:layout_toRightOf="@id/textdigital"
                android:src="@drawable/signgreater"/>
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/finance_rela"
             android:layout_below="@id/digital_rela"
             android:layout_marginLeft="10dp"
             android:layout_marginRight="10dp"
             android:layout_width="match_parent"
             android:layout_height="100dp"
             android:orientation="horizontal"
             android:layout_marginTop="10dp"
             android:background="#FDFDFD"
             >
             <ImageView
                 android:id="@+id/image_finance"
                 android:layout_marginTop="20dp"
                 android:layout_width="80dp"
                 android:layout_height="70dp"
                 android:src="@drawable/finance"
                 android:layout_marginBottom="20dp"
                 android:layout_marginLeft="20dp"/>
         <TextView
             android:id="@+id/textfinance"
             android:layout_toRightOf="@+id/image_finance"
             android:layout_width="160dp"
             android:layout_marginTop="30dp"
             android:textStyle="bold"
             android:layout_height="wrap_content"
             android:textColor="@color/skyblue"
             android:text="FINANCE"
             android:gravity="center"
             android:padding="10dp"
             android:textSize="20dp"
             />
             <ImageView
                 android:id="@+id/finance_sign"
                 android:layout_marginTop="28dp"
                 android:background="@drawable/circle_background"
                 android:backgroundTint="@color/colorPrimary"
                 android:tint="#FDFDFD"
                 android:layout_width="40dp"
                 android:layout_marginLeft="10dp"
                 android:layout_marginRight="10dp"
                 android:layout_height="40dp"
                 android:layout_toRightOf="@id/textfinance"
                 android:src="@drawable/signgreater"/>
         </RelativeLayout>

         <RelativeLayout
             android:id="@+id/hr_rela"
             android:layout_width="match_parent"
             android:layout_marginRight="10dp"
             android:layout_height="100dp"
             android:layout_below="@id/finance_rela"
             android:layout_marginTop="10dp"
             android:background="#FDFDFD"
             android:layout_marginLeft="10dp"
             android:orientation="horizontal">

             <ImageView
                 android:id="@+id/iamge_hr"
                 android:layout_width="80dp"
                 android:layout_height="70dp"
                 android:layout_marginTop="20dp"
                 android:layout_marginLeft="20dp"
                 android:layout_marginBottom="20dp"
                 android:src="@drawable/hr" />

             <TextView
                 android:id="@+id/texthr"
                 android:layout_toRightOf="@id/iamge_hr"
                 android:layout_width="160dp"
                 android:textColor="@color/skyblue"
                 android:textStyle="bold"
                 android:layout_height="wrap_content"
                 android:gravity="center"
                 android:layout_marginTop="30dp"
                 android:padding="10dp"
                 android:text="HR"
                 android:textSize="20dp" />
             <ImageView
                 android:id="@+id/hr_sign"
                 android:layout_marginTop="28dp"
                 android:background="@drawable/circle_background"
                 android:backgroundTint="@color/colorPrimary"
                 android:tint="#FDFDFD"
                 android:layout_width="40dp"
                 android:layout_marginLeft="10dp"
                 android:layout_marginRight="10dp"
                 android:layout_height="40dp"
                 android:layout_toRightOf="@id/texthr"
                 android:src="@drawable/signgreater"/>

         </RelativeLayout>
        <RelativeLayout
            android:id="@+id/seo_rela"
            android:layout_below="@id/hr_rela"
            android:layout_width="match_parent"
            android:background="#FDFDFD"
            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">
            <ImageView
                android:id="@+id/image_seo"
                android:layout_width="80dp"
                android:layout_marginTop="20dp"
                android:layout_height="70dp"
                android:src="@drawable/seotrans"
                android:layout_marginLeft="20dp"
                android:layout_marginBottom="20dp"/>
        <TextView
            android:id="@+id/textseo"
            android:layout_toRightOf="@+id/image_seo"
            android:layout_width="160dp"
            android:layout_marginTop="30dp"
            android:layout_height="wrap_content"
            android:text="SEO"
            android:textColor="@color/skyblue"
            android:textStyle="bold"
            android:gravity="center"
            android:padding="10dp"
            android:textSize="20dp"
            />
            <ImageView
                android:id="@+id/seo_sign"
                android:layout_marginTop="28dp"
                android:background="@drawable/circle_background"
                android:backgroundTint="@color/colorPrimary"
                android:tint="#FDFDFD"
                android:layout_width="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_height="40dp"
                android:layout_toRightOf="@id/textseo"
                android:src="@drawable/signgreater"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/accounting_rela"
            android:layout_below="@id/seo_rela"
            android:background="#FDFDFD"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            >
            <ImageView
                android:id="@+id/image_accounting"
                android:layout_width="80dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:src="@drawable/accounting"
                android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/textaccounting"
            android:layout_toRightOf="@+id/image_accounting"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="ACCOUNTING"
            android:gravity="center"
            android:textColor="@color/skyblue"
            android:textStyle="bold"
            android:layout_marginTop="30dp"
            android:padding="10dp"
            android:textSize="20dp"
            />
            <ImageView
                android:id="@+id/accounting_sign"
                android:layout_marginTop="28dp"
                android:background="@drawable/circle_background"
                android:backgroundTint="@color/colorPrimary"
                android:tint="#FDFDFD"
                android:layout_width="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_height="40dp"
                android:layout_toRightOf="@id/textaccounting"
                android:src="@drawable/signgreater"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/tally_rela"
             android:layout_width="match_parent"
             android:layout_height="100dp"
            android:layout_marginBottom="20dp"
            android:layout_below="@id/accounting_rela"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:background="#FDFDFD"
            >
             <ImageView
                 android:id="@+id/image_tally"
                 android:layout_width="80dp"
                 android:layout_height="70dp"
                 android:src="@drawable/tally"
                 android:layout_marginTop="20dp"
                 android:padding="5dp"
                 android:layout_marginBottom="20dp"
                 android:layout_marginLeft="20dp"/>
             <TextView
                 android:id="@+id/texttally"
                 android:layout_toRightOf="@+id/image_tally"
                 android:layout_width="160dp"
                 android:layout_height="wrap_content"
                 android:text="TALLY"
                 android:textColor="@color/skyblue"
                 android:textStyle="bold"
                 android:gravity="center"
                 android:padding="10dp"
                 android:layout_marginTop="30dp"
                 android:textSize="20dp"
                 />
            <ImageView
                android:id="@+id/tally_sign"
                android:layout_marginTop="28dp"
                android:background="@drawable/circle_background"
                android:backgroundTint="@color/colorPrimary"
                android:tint="#FDFDFD"
                android:layout_width="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_height="40dp"
                android:layout_toRightOf="@id/texttally"
                android:src="@drawable/signgreater"/>
         </RelativeLayout>
    </RelativeLayout>
</ScrollView>